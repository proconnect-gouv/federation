<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
	<comment>SWModule sign with JKS.</comment>
	<!-- Certificates -->
	<entry key="check.certificate.validity.period">true</entry>

	<!-- Signature with RSA -->
	<entry key="signature.algorithm">http://www.w3.org/2007/05/xmldsig-more#sha256-rsa-MGF1</entry>
	<entry key="metadata.signature.algorithm">http://www.w3.org/2007/05/xmldsig-more#sha256-rsa-MGF1</entry> 	

    <entry key="signature.algorithm.whitelist">
        http://www.w3.org/2007/05/xmldsig-more#sha256-rsa-MGF1;
        http://www.w3.org/2007/05/xmldsig-more#sha384-rsa-MGF1;
        http://www.w3.org/2007/05/xmldsig-more#sha512-rsa-MGF1;
        http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha256;
        http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha384;
        http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha512
    </entry>

	<entry key="keyStorePath">../keystore/keystore-eidas-service-fr.jks</entry>
	<entry key="keyStorePassword">local-demo</entry>
	<entry key="keyPassword">local-demo</entry>
	<entry key="issuer">CN=eidas-service-fr, OU=FC, O=DINUM, L=Paris, ST=France, C=FR</entry>
	<entry key="serialNumber">2917c52e79a629fcc647a230f8b428797d177b59</entry>
	<entry key="keyStoreType">JKS</entry>

	<entry key="metadata.keyStorePath">../keystore/keystore-eidas-metadata-fr.jks</entry>
	<entry key="metadata.keyStorePassword">local-demo</entry>
	<entry key="metadata.keyPassword">local-demo</entry>
	<entry key="metadata.issuer">CN=eidas-metadata-fr, OU=FC, O=DINUM, L=Paris, ST=France, C=FR</entry>
	<entry key="metadata.serialNumber">2917c52e79a629fcc647a230f8b428797d177b58</entry>
	<entry key="metadata.keyStoreType">JKS</entry>

</properties>
