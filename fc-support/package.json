{
  "name": "@fc/support",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "parcel build -d ./dist/client ./public/javascript/main.js && cp ./public/favicon.ico ./dist/client/ && cp -R ./public/images ./dist/client/ ",
    "build": "tsc -p tsconfig.build.json",
    "fixtures:load": "fixtures ../shared/fixtures --config src/config/database.ts --require=ts-node/register --require=tsconfig-paths/register",
    "migrations:run": "yarn typeorm migration:run && echo '\n\n ***Make sure to load fixtures and create backup DB for E2E***\n from you host run:\n > ../shared/cypress/support/db.sh support create'",
    "format": "prettier --write \"src/**/*.ts\"",
    "prestart": "yarn run prebuild",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ../node_modules/typeorm/cli.js --config src/config/database.ts",
    "console": "node dist/console.js",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts"
  },
  "dependencies": {
    "@fc/shared": "^0.0.1",
    "@nest-middlewares/csurf": "^6.0.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.2",
    "bootstrap": "^4.3.1",
    "clipboard": "^2.0.4",
    "commander": "^4.0.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "express-flash": "0.0.2",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.16.1",
    "font-awesome": "^4.7.0",
    "helmet": "^3.18.0",
    "jquery": "^3.4.1",
    "lightpick": "^1.3.4",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "nestjs-config": "^1.4.1",
    "nestjs-console": "^1.2.0",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "sqlite3": "^4.0.8",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^12.6.8",
    "@types/supertest": "^2.0.7",
    "@types/xml2js": "^0.4.4",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "parcel-bundler": "^1.12.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "typeorm-fixtures-cli": "^1.1.5",
    "typescript": "3.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
