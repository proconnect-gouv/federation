{
  "name": "@fc/stats",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "parcel build -d ./dist/client ./public/javascript/main.js && cp ./public/favicon.ico ./dist/client/ && cp -R ./public/images ./dist/client/ ",
    "build": "tsc -p tsconfig.build.json",
    "fixtures:load": "fixtures ../shared/fixtures --config src/config/database.ts --require=ts-node/register --require=tsconfig-paths/register",
    "fixtures:es": "curl -XPUT http://localhost:9200/_bulk --data-binary @fixtures/bulk.json",
    "migrations:run": "yarn typeorm migration:run && echo '\n\n ***Make sure to load fixtures and create backup DB for E2E***\n from you host run:\n > ../shared/cypress/support/db.sh stats create'",
    "format": "prettier --write \"src/**/*.ts\"",
    "prestart": "yarn run prebuild",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ../node_modules/typeorm/cli.js --config src/config/database.ts",
    "console": "node dist/console.js",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts"
  },
  "resolutions": {
    "pg": "8.5.1",
    "typeorm": "0.2.32"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.15.0",
    "@fc/shared": "^0.0.1",
    "@nest-middlewares/csurf": "^6.0.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/elasticsearch": "^6.0.1",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.13.0",
    "commander": "^4.0.0",
    "cookie-parser": "^1.4.4",
    "csv-stringify": "^5.3.0",
    "dotenv": "^8.0.0",
    "ejs": "^3.1.0",
    "elasticsearch": "^16.3.0",
    "elasticsearch-scroll-stream": "^1.2.0",
    "express-flash": "0.0.2",
    "express-session": "^1.16.1",
    "font-awesome": "^4.7.0",
    "helmet": "^3.23.3",
    "jquery": "^3.4.1",
    "lightpick": "^1.3.4",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "nestjs-config": "^1.4.1",
    "nestjs-console": "^1.2.0",
    "pg": "^8.0.3",
    "popper.js": "^1.15.0",
    "query-string": "^6.8.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "typeorm": "0.2.32"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/elasticsearch": "^5.0.34",
    "@types/express": "^4.16.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^12.6.8",
    "@types/supertest": "^2.0.7",
    "cypress": "9.2.0",
    "jest": "^27.0.0",
    "less": "~3.12.0",
    "parcel-bundler": "1.12.5",
    "supertest": "^3.4.1",
    "ts-jest": "^27.0.0",
    "ts-node": "8.1.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm-fixtures-cli": "^1.1.5",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
