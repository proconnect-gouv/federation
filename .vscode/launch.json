{
  "version": "0.2.0",
  "setupCommands": [
    {
      "ignoreFailures": true
    }
  ],
  "compounds": [
    {
      "name": "FCP-HIGH - ALL",
      "configurations": [
        "FCP - CORE-HIGH",
        "FCP - FSP1-HIGH",
        "FCP - FSP2-HIGH",
        "FCP - FSP3-HIGH",
        "FCP - Eidas-Bridge",
      ],
    },
    {
      "name": "FCP-LOW - ALL",
      "configurations": [
        "FCP - CORE-LOW",
        "FCP - FSP1-LOW",
        "FCP - FIP1-LOW",
        "FCP - FIP2-LOW",
        "FCP - FIP3-LOW",
      ],
    },
  ],
  "serverReadyAction": {
    "pattern": "listening on.* (https?://\\S+|[0-9]+)",
    "uriFormat": "http://localhost:%s",
    "action": "debugWithChrome"
  },
  "configurations": [
    {
      "name": "FCP - CORE-HIGH",
      "type": "node",
      "request": "attach",
      "port": 9235,
      "address": "localhost",
      "internalConsoleOptions": "openOnFirstSessionStart",
      "protocol": "inspector",
      "localRoot": "${workspaceFolder}/back",
      "remoteRoot": "/var/www/app",
      "outFiles": ["${workspaceFolder}/back/dist/apps/core-fcp/**/*.js"],
      "skipFiles": [
        "${workspaceFolder}/back/node_modules/**/*.js",
        "<node_internals>/**/*.js"
      ],
      "cwd": "${workspaceFolder}/back",
      "restart": true,
      "sourceMaps": true,
    },
    {
      "name": "FCP - FSP1-HIGH",
