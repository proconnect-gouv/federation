eidas-bridge/build-image:
  extends: .base-build
  variables:
    APP: eidas-bridge
    MSG: "environments/pa4-integ/group_vars/all/versions.yml\neidas_bridge_version:"

eidas-bridge/tag-to-prod-image:
  extends: .base-tag-to-prod
  variables:
    DOCKER_IMAGE: eidas-bridge
    MSG: "environments/pa4-preprod/group_vars/all/versions.yml\nenvironments/pa4-prod/group_vars/all/versions.yml\neidas_bridge_version:"  
  needs:
    - job: eidas-bridge/build-image
