include: '/back/.gitlab-build-template.yml'

fc_legacy/fsp1-legacy-build-image:
  extends: .base-build
  variables:
    APP: mock-service-provider-fcp-legacy
    MSG: "environments/outscale-tools/group_vars/all/versions.yml\nfsp1_legacy_version:"

fc_legacy/fsp1-legacy-tag-to-prod-image:
  extends: .base-tag-to-prod
  variables:
    DOCKER_IMAGE: mock-service-provider-fcp-legacy
    MSG: "environments/outscale-tools/group_vars/all/versions.yml\nfsp1_legacy_version:"
  needs:
    - job: fc_legacy/fsp1-legacy-build-image
