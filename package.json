    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.9.5",
    "node-mailjet": "^3.3.1",
    "oidc-provider": "6.25.0",
    "openid-client": "3.15.0",
    "pino": "^5.17.0",
    "pkcs11js": "^1.0.19",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "uuid": "^7.0.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.7",
    "@stryker-mutator/core": "^3.0.0",
    "@stryker-mutator/jest-runner": "^3.1.0",
    "@stryker-mutator/typescript": "^3.1.0",
    "@types/express": "^4.17.2",
    "@types/ioredis": "^4.14.9",
    "@types/jest": "25.1.2",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^25.1.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.16.10",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
