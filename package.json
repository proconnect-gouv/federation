{
  "name": "fc",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:mutate": "stryker run",
    "test:e2e": "cypress run",
    "doc:api": "typedoc"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/platform-express": "^7.0.7",
    "@shelf/jest-mongodb": "^1.1.3",
    "base64url": "^3.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "deep-freeze": "^0.0.1",
    "express-session": "^1.17.0",
    "jose": "1.25.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.5",
    "oidc-provider": "6.23.3",
    "openid-client": "3.14.1",
    "pino": "^5.17.0",
    "pkcs11js": "^1.0.19",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.7",
    "@stryker-mutator/core": "^2.5.0",
    "@stryker-mutator/jest-runner": "^2.5.0",
    "@stryker-mutator/typescript": "^2.5.0",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.2",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^25.1.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.16.10",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@fc/rnipp/(.*)": "<rootDir>/libs/rnipp/src/$1",
      "@fc/rnipp": "<rootDir>/libs/rnipp/src",
      "@fc/identity-management/(.*)": "<rootDir>/libs/identity-management/src/$1",
      "@fc/identity-management": "<rootDir>/libs/identity-management/src",
      "@fc/id-pmanagement/(.*)": "<rootDir>/libs/id-pmanagement/src/$1",
      "@fc/id-pmanagement": "<rootDir>/libs/id-pmanagement/src",
      "@fc/spmanagement/(.*)": "<rootDir>/libs/spmanagement/src/$1",
      "@fc/spmanagement": "<rootDir>/libs/spmanagement/src",
      "@fc/logger/(.*)": "<rootDir>/libs/logger/src/$1",
      "@fc/logger": "<rootDir>/libs/logger/src",
      "@fc/consent-management/(.*)": "<rootDir>/libs/consent-management/src/$1",
      "@fc/consent-management": "<rootDir>/libs/consent-management/src",
      "@fc/oidc-provider/(.*)": "<rootDir>/libs/oidc-provider/src/$1",
      "@fc/oidc-provider": "<rootDir>/libs/oidc-provider/src",
      "@fc/oidc-client/(.*)": "<rootDir>/libs/oidc-client/src/$1",
      "@fc/oidc-client": "<rootDir>/libs/oidc-client/src",
      "@fc/cryptography/(.*)": "<rootDir>/libs/cryptography/src/$1",
      "@fc/cryptography": "<rootDir>/libs/cryptography/src",
      "@fc/core-fcp/(.*)": "<rootDir>/libs/core-fcp/src/$1",
      "@fc/core-fcp": "<rootDir>/libs/core-fcp/src",
      "@fc/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@fc/common": "<rootDir>/libs/common/src",
      "@fc/siren/(.*)": "<rootDir>/libs/siren/src/$1",
      "@fc/siren": "<rootDir>/libs/siren/src",
      "@fc/cryptography-gateway-low/(.*)": "<rootDir>/libs/cryptography-gateway-low/src/$1",
      "@fc/cryptography-gateway-low": "<rootDir>/libs/cryptography-gateway-low/src",
      "@fc/cryptography-gateway-high/(.*)": "<rootDir>/libs/cryptography-gateway-high/src/$1",
      "@fc/cryptography-gateway-high": "<rootDir>/libs/cryptography-gateway-high/src",
      "@fc/config/(.*)": "<rootDir>/libs/config/src/$1",
      "@fc/config": "<rootDir>/libs/config/src",
      "@fc/error/(.*)": "<rootDir>/libs/error/src/$1",
      "@fc/error": "<rootDir>/libs/error/src",
      "@fc/mongoose/(.*)": "<rootDir>/libs/mongoose/src/$1",
      "@fc/mongoose": "<rootDir>/libs/mongoose/src",
      "@fc/redis/(.*)": "<rootDir>/libs/redis/src/$1",
      "@fc/redis": "<rootDir>/libs/redis/src"
    },
    "preset": "ts-jest"
  }
}