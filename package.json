  "scripts": {
    "build": "nest build",
    "build:core-fcp-high": "nest build core-fcp-high",
    "build:csmr-hsm-high": "nest build csmr-hsm-high",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod:core-fcp-high": "node dist/instances/core-fcp-high/main.js > /tmp/.pm2/logs/corev2-out-0.log 2> /tmp/.pm2/logs/corev2-error-0.log",
    "start:prod:csmr-hsm-high": "node dist/instances/csmr-hsm-high/main.js > /tmp/.pm2/logs/csmr-hsm-high-out-0.log 2> /tmp/.pm2/csmr-hsm-high-error-0.log",
    "start:prod:mock-service-provider-high": "node dist/instances/mock-service-provider-high/main.js > /tmp/.pm2/logs/mock-service-provider-high-out-0.log 2> /tmp/.pm2/mock-service-provider-high-error-0.log",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:mutate": "stryker run",
    "test:e2e:core-fcp-high": "cypress run -P instances/core-fcp-high",
    "doc:api": "typedoc"
  },
  "resolutions": {
    "cookie-signature": "1.1.0",
    "jose": "1.27.3"
  },
