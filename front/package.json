{
  "name": "fc-front",
  "version": "0.1.0",
  "description": "FC Front Apps mono repository",
  "private": true,
  "engine": "^14.0.0",
  "workspaces": [
    "apps/*",
    "libs/*",
    "instances/*",
    "tests-utils"
  ],
  "scripts": {
    "start:ud": "cd instances/user-dashboard && yarn start",
    "start:partners-fca": "cd instances/partners-fca && yarn start",
    "start:partners-fcp": "cd instances/partners-fcp && yarn start",
    "start:fca": "cd instances/agent-connect && yarn start",
    "start:exploit-fcp-high": "cd instances/exploit-fcp-high && yarn start",
    "start:exploit-fca-low": "cd instances/exploit-fca-low && yarn start",
    "start:storybook": "start-storybook -p 3000",
    "build:fca-low-front": "cd instances/agent-connect && ../../scripts/yarn-build agent-connect",
    "build:storybook": "build-storybook",
    "cleanup": "./scripts/yarn-cleanup",
    "format:js": "prettier --write \"{apps,instances,libs}/**/*.(ts|tsx)\"",
    "format:css": "stylelint \"{apps,instances,libs}/**/*.(scss|css|sass)\" --fix",
    "format": "yarn format:css && yarn format:js",
    "lint:css": "stylelint \"{apps,instances,libs}/**/*.(scss|css|sass)\"",
    "lint:js": "eslint ./apps ./instances ./libs",
    "lint": "yarn lint:css && yarn lint:js",
    "prettier": "prettier --check \"{apps,instances,libs}/**/*.(ts|tsx|scss)\"",
    "test": "NODE_ENV=test jest --config=./jest.config.js",
    "test:cov": "yarn test --coverage",
    "serve:ud": "serve --config=/var/www/app/serve.json -n -d instances/user-dashboard/build/",
    "serve:fca": "serve --config=/var/www/app/serve.json -n -d instances/agent-connect/build/",
    "serve:exploit-fcp-high": "serve --config=/var/www/app/serve.json -n -d instances/exploit-fcp-high/build/",
    "serve:exploit-fca-low": "serve --config=/var/www/app/serve.json -n -d instances/exploit-fca-low/build/"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@craco/craco": "^7.0.0-alpha.7",
    "@fc/tests-utils": "^0.1.0",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.2.4",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.range": "^3.2.7",
    "@types/luxon": "^3.1.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.6",
    "@types/react-final-form-listeners": "^1.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-responsive": "^8.0.4",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-testing-library": "^5.2.1",
    "http-proxy-middleware": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-extended": "^3.1.0",
    "jest-mock": "^29.3.1",
    "jest-mock-axios": "^4.5.0",
    "jest-svg-transformer": "^1.0.0",
    "prettier": "^2.1.2",
    "react-app-rewire-alias": "1.1.7",
    "react-app-rewired": "^2.1.8",
    "react-refresh-typescript": "^2.0.2",
    "react-scripts": "5.0.1",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.45.0",
    "serve": "^14.1.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-jest": "^29.0.3",
    "type-fest": "^3.5.0",
    "typescript": "^4.4.4",
    "web-vitals": "^3.1.0"
  },
  "resolutions": {
    "eslint-plugin-react": "7.31.11",
    "react-error-overlay": "6.0.11",
    "babel-loader": "9.1.0",
    "@types/react": "18.0.26"
  },
  "dependencies": {
    "@types/diacritics": "^1.3.1",
    "diacritics": "^1.3.0"
  }
}
