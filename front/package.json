{
  "name": "fc-front",
  "version": "0.1.0",
  "description": "FC Front Apps mono repository",
  "private": true,
  "engine": "^14.0.0",
  "workspaces": [
    "apps/*",
    "libs/*",
    "tests-utils"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "start:ud": "cd apps/user-dashboard && yarn start",
    "start:partners-fca": "cd apps/partners-fca && yarn start",
    "start:partners-fcp": "cd apps/partners-fcp && yarn start",
    "start:fca": "cd apps/agent-connect && yarn start",
    "start:exploit-fcp-high": "cd apps/exploit-fcp-high && yarn start",
    "start:exploit-fca-low": "cd apps/exploit-fca-low && yarn start",
    "start:storybook": "start-storybook -p 3000",
    "build:fca-low-front": "cd apps/agent-connect && ../../scripts/yarn-build agent-connect",
    "build:storybook": "build-storybook",
    "cleanup": "./scripts/yarn-cleanup",
    "format:js": "prettier --write \"{apps,libs}/**/*.(ts|tsx)\"",
    "format:css": "stylelint \"{apps,libs}/**/*.(scss|css|sass)\" --fix",
    "format": "yarn format:css && yarn format:js",
    "lint:css": "stylelint \"{apps,libs}/**/*.(scss|css|sass)\"",
    "lint:js": "eslint ./apps ./libs",
    "lint": "yarn lint:css && yarn lint:js",
    "prettier": "prettier --check \"{apps,libs}/**/*.(ts|tsx|scss)\"",
    "test": "NODE_ENV=test jest --config=./jest.config.js",
    "test:cov": "yarn test --coverage",
    "serve:ud": "serve --config=/var/www/app/serve.json -n -d apps/user-dashboard/build/",
    "serve:fca": "serve --config=/var/www/app/serve.json -n -d apps/agent-connect/build/",
    "serve:exploit-fcp-high": "serve --config=/var/www/app/serve.json -n -d apps/exploit-fcp-high/build/",
    "serve:exploit-fca-low": "serve --config=/var/www/app/serve.json -n -d apps/exploit-fca-low/build/"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^28.1.2",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^28.1.2",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-testing-library": "^5.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-mock": "^28.1.1",
    "jest-mock-axios": "^4.5.0",
    "jest-svg-transformer": "^1.0.0",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.45.0",
    "serve": "^13.0.2",
    "ts-jest": "^28.0.5",
    "type-fest": "^2.5.0",
    "typescript": "^4.4.4",
    "web-vitals": "^2.1.4"
  },
  "resolutions": {
    "eslint-plugin-react": "7.28.0",
    "react-error-overlay": "6.0.9",
    "babel-loader": "8.1.0",
    "@types/react": "18.0.14" 
  }
}
